{if((name!="")&&(name!=null)){this.TaskInfo.Name=name;if(this.Chart._showTreePanel)
{this.cTaskNameItem[0].innerHTML=name;this.cTaskNameItem[0].title=name;this.checkWidthTaskNameItem();}
