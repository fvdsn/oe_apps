dhtmlxEventable=function(obj){obj.attachEvent=function(name,catcher,callObj){name='ev_'+name.toLowerCase();if(!this[name])
this[name]=new this.eventCatcher(callObj||this);return(name+':'+this[name].addEvent(catcher));}
