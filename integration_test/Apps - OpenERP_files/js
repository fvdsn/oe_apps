item.firstChild.nodeValue=paramValue}
dtmlXMLLoaderObject.prototype.doXSLTransToObject=function(xslDoc,xmlDoc){if(!xslDoc)
xslDoc=this.xslDoc;if(xslDoc.responseXML)
xslDoc=xslDoc.responseXML
